 <div class="container">
   <div class="row">
      <form class="col s9 action="search/" method="GET"">
        <div class="row">
          <div class="input-field col s6">
            <i class="material-icons prefix">account_circle</i>
            <input id="icon_prefix" type="text" class="validate" name="search"/>
            <label for="icon_prefix">Username</label>
            <button type="submit" class="waves-effect waves-light btn">Search</button>
          </div>
        </div>
      </form>
    </div>
   <ul class="collection">
     <% @search_users.each do |users| %>
     <li class="collection-item avatar">
       <img src="<%= users.url%>" alt="" class="circle">
       <span class="title"><%= users.username %></span>
       <p><%= users.bio %><br>
       </p>
       <a class="waves-effect waves-light btn secondary-content">Follow</a>
     </li>
     <% end %>
   </ul>

 </div>
